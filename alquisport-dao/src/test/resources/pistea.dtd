<!ELEMENT dataset (
    audit_trail*,
    calendar*,
    company*,
    coupon*,
    coupon_used*,
    court*,
    court_feature*,
    days*,
    feature*,
    hibernate_sequences*,
    image*,
    message*,
    month*,
    municipality*,
    province*,
    rent*,
    rental_client*,
    role*,
    schedule*,
    sport_event_rentals*,
    sport_facility*,
    sport_facility_features*,
    sport_facility_image*,
    sport_facility_member*,
    sports_event*,
    user*,
    user_role*,
    year*,
    zone*)>

<!ELEMENT audit_trail EMPTY>
<!ATTLIST audit_trail
    ID CDATA #REQUIRED
    DATE CDATA #IMPLIED
    DESCRIPTION CDATA #IMPLIED
>

<!ELEMENT calendar EMPTY>
<!ATTLIST calendar
    ID CDATA #REQUIRED
>

<!ELEMENT company EMPTY>
<!ATTLIST company
