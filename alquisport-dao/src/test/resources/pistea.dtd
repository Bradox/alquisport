    CIF CDATA #REQUIRED
    ADDRESS CDATA #IMPLIED
    TYPE CDATA #IMPLIED
    ZIP_CODE CDATA #IMPLIED
    NAME CDATA #REQUIRED
    ID_MUNICIPALITY CDATA #IMPLIED
    ID_PROVINCE CDATA #IMPLIED
    ID_ZONE CDATA #IMPLIED
>

<!ELEMENT coupon EMPTY>
<!ATTLIST coupon
    ID CDATA #REQUIRED
    CODE CDATA #REQUIRED
    DISCOUNT_QUANTITY CDATA #REQUIRED
    DISCOUNT_TYPE CDATA #REQUIRED
    MAX_TIMES_USE CDATA #REQUIRED
    TIMES_USED CDATA #REQUIRED
>

<!ELEMENT coupon_used EMPTY>
<!ATTLIST coupon_used
    ID CDATA #REQUIRED
    ID_USER CDATA #IMPLIED
    ID_COUPON CDATA #IMPLIED
